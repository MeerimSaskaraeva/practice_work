--Создать таблицу "users",таблица users имеет поля id, login, salary, age, working_year, description (и другие при необходимости).
create table users (
                       id INT,
                       email VARCHAR(50),
                       age INT,
                       working_year DATE,
                       description VARCHAR(150)
);
